{
    "name": "<%- projectName %>",
    "version": "0.0.1",
    "description": "<%- projectDescription %>",
    "repository": "<%- repository %>",
    "homepage": "<%- repositoryHomepageUrl %>",
    "bugs": "<%- repositoryBugsUrl %>",
    "author": "<%- author %>",
    "keywords": [
        "sprout-template"
    ],
    "devDependencies": {
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-prettier": "^2.6.0",
        "jest": "^22.4.3",
        "prettier": "^1.12.1",
        "shelljs": "^0.8.1",
        "sprout": "^1.2.1"
    },
    "dependencies": {
        "gh-got": "^7.0.0",
        "underscore.string": "^3.3.4"
    },
    "main": "init.js",
    "private": true,
    "license": "<%- license %>",
    "eslintConfig": {
        "env": {
            "node": true,
            "es6": true
        },
        "extends": [
            "plugin:prettier/recommended"
        ]
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "avoid",
        "proseWrap": "always",
        "requirePragma": false,
        "insertPragma": false,
        "semi": false
    },
    "scripts": {
        "test": "jest --coverage"
    }
}
